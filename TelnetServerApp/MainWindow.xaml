<Window x:Class="TelnetServerApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TelnetServerApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="100" Width="100">
    <StackPanel>
        <TextBlock>telnet</TextBlock>
        <Button Command="{Binding Start}">Start</Button>
        <Button Command="{Binding Stop}">Stop</Button>
    </StackPanel>
</Window>
